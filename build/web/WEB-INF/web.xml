<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <listener>
        <description>ServletContextListener</description>
        <listener-class>newpackage.ConnServletListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>FilterServlet</servlet-name>
        <servlet-class>newpackage.FilterUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UdpServlet</servlet-name>
        <servlet-class>newpackage.UdpServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TcpServlet</servlet-name>
        <servlet-class>newpackage.TcpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FilterServlet</servlet-name>
        <url-pattern>/FilterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UdpServlet</servlet-name>
        <url-pattern>/UdpServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TcpServlet</servlet-name>
        <url-pattern>/TcpServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
